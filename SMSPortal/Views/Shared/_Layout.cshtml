@{
    if (!Request.Url.AbsolutePath.Trim().Equals("/Home/Logout", StringComparison.CurrentCultureIgnoreCase))
    {
        Response.AddHeader("Cache-Control", "no-cache; private; no-store; must-revalidate; max-stale=0; post-check=0; pre-check=0; max-age=0");
        Response.AddHeader("Pragma", "no-cache");
    }
    Response.Cache.SetAllowResponseInBrowserHistory(false);
    Response.Cache.SetExpires(DateTime.Now.AddSeconds(5));
    Response.Cache.SetExpires(DateTime.UtcNow.AddSeconds(5));
    Response.Cache.SetNoServerCaching();
    Response.Cache.SetValidUntilExpires(true);
    Response.Cache.SetMaxAge(TimeSpan.FromSeconds(5));
    Response.Cache.SetProxyMaxAge(TimeSpan.Zero);
}

