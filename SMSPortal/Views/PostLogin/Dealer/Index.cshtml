<script src="~/Scripts/Module/Dealer/Dealer_events.js"></script>
<div class="content-wrapper">
    @*@{Html.RenderPartial("_ServerMessage");}*@
    <section class="content-header">
        <h1>
            Dealer Master
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
            <li class="active">Admin</li>
            <li class="active">Dealer</li>
        </ol>
    </section>
    <form id="frmDealerMaster">
        <section class="content">
            <div class="box box-primary">
                <div class="row">
                    <div class="col-md-12">
                        <div class="box-header with-border">
                            <i class="fa fa-chevron-left" style="margin-right: 5px; cursor: pointer;font-size:22px; "></i>
                            <h3 class="box-title">Dealer </h3>
                        </div>
                        <div class="box-body">
                            <div class="row">
                                <div class="col-lg-5">
                                    <div class="row">
                                        <div class="col-lg-8">
                                            <div class="form-group">
                                                <label style="color: #333">Dealer Name</label>
                                                <input type="text" class="form-control input-sm" name="" id="txtDealer_Name" placeholder="Enter your name" value="" maxlength="20">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label style="color: #333">Brand</label>
                                                <select id="drpBrand_Category" class="form-control" name="">
                                                    <option value="">-Select Brand-</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label style="color: #333">Dealer Percentage Share</label>
                                                <input type="text" class="form-control input-sm" name="" id="txtDealerPercentageShare" placeholder="Dealer percentage share" value="" maxlength="20">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label style="color: #333">Brand Percentage Share</label>
                                                <input type="text" class="form-control input-sm" name="" id="txtBrandPercentageShare" placeholder="Brand percentage share" value="" maxlength="20">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <br />
                                                <label>
                                                    <input type="checkbox" name="chkStatus" checked="checked" />
                                                    &nbsp;&nbsp;&nbsp;&nbsp; Active
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-5">
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label style="color: #333">Address</label>
                                                <textarea class="form-control"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label style="color: #333">City</label>
                                                <input type="text" class="form-control input-sm" name="" id="txtcity" placeholder="Enter city" value="" maxlength="20">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label style="color: #333">State</label>
                                                <select id="drpState" class="form-control" name="">
                                                    <option value="">-Select State-</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label style="color: #333">Pincode</label>
                                                <input type="text" class="form-control input-sm" name="" id="txtPincode" placeholder="Enter pincode" value="" maxlength="20">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label style="color: #333">Contact No 1</label>
                                                <input type="text" class="form-control input-sm" name="" id="txtContactNo1" placeholder="Enter your contact" value="" maxlength="20">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label style="color: #333">Contact No 2</label>
                                                <input type="text" class="form-control input-sm" name="" id="txtContactNo2" placeholder="Enter your alternative contact" value="" maxlength="20">

                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label style="color: #333">Email</label>
                                                <input type="text" class="form-control input-sm" name="" id="txtEmail" placeholder="Enter email " value="" maxlength="20">
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>


                        </div>

                        <div class="box-footer">
                            <div class="btn-group">
                                <button type="submit" id="btnSave" class="btn btn-success btn-sm">Save</button>
                                <button type="reset" id="btnReset" class="btn btn-warning btn-sm">Reset</button>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
</div>


