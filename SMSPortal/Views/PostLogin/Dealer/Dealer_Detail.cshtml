@model SMSPortal.Models.PostLogin.DealerViewModel
@using SMSPortal.Common;

<script src="~/Scripts/Module/Dealer/search_dealer_events.js"></script>

<script src="~/Scripts/Module/Dealer/search_dealer_methods.js"></script>


    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        @{Html.RenderPartial("_ServerMessage");}
        <section class="content-header">
            <h1>
                Dealer Master
                <small></small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
                <li><a href="#"><i class="fa fa-dashboard"></i>Admin</a></li>
                <li class="active">Dealer</li>
            </ol>
        </section>

        <div class="row">

            <div class="col-xs-12">

                <div class="box">

                    <div class="box-body">

                        <div class="row">

                            <div class="col-md-12"> 

                                <form id="frmDealerMaster">

                                    <table border="1" style="width:100%">

                                        <tr>

                                            <th style="padding: 5px; text-align: left; width:15%">Dealer Name : </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                @*<div class="form-group">*@

                                                <label style="color: #333" for="txtCustomer_Name">@Model.Dealer.Dealer_Name</label>

                                                @*</div>*@

                                            </td>

                                        </tr>

                                        <tr>

                                            <th style="padding: 5px; text-align: left; width:15%">Brand : </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                @*<div class="form-group">*@

                                                <label style="color: #333" for="txtCustomer_Address">@Model.Dealer.Brand_Name</label>

                                                @*</div>*@

                                            </td>

                                        </tr>

                                        <tr>

                                            <th style="padding: 5px; text-align: left; width:15%">Address : </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                @*<div class="form-group">*@

                                                <label style="color: #333" for="txtTel_No">@Model.Dealer.Address</label>

                                                @*</div>*@

                                            </td>

                                        </tr>

                                        <tr>

                                            <th style="padding: 5px; text-align: left; width:15%">City : </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                @*<div class="form-group">*@

                                                <label style="color: #333" for="txtEmail">@Model.Dealer.City</label>

                                                @*</div>*@

                                            </td>

                                        </tr>

                                        <tr>

                                            <th style="padding: 5px; text-align: left; width:15%">State :  </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                @*<div class="form-group">*@

                                                <label style="color: #333" for="txtContact_Person">@Model.Dealer.State</label>

                                                @*</div>*@

                                            </td>

                                        </tr>

                                        <tr>

                                            <th style="padding: 5px; text-align: left; width:15%">Pincode : </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                @*<div class="form-group">*@

                                                <label style="color: #333" for="txtContact_Person_Tel_No"> @Model.Dealer.Pincode</label>

                                                @*</div>*@

                                            </td>

                                        </tr>

                                        <tr>

                                            <th style="padding: 5px; text-align: left; width:15%">Contact No 1 : </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                @*<div class="form-group">*@

                                                <label style="color: #333" for="txtContact_Person_Mobile_No"> @Model.Dealer.Contact_No_1</label>

                                                @*</div>*@

                                            </td>

                                        </tr>

                                        <tr>

                                            <th style="padding: 5px; text-align: left; width:15%">Contact No 2 : </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                @*<div class="form-group">*@

                                                <label style="color: #333" for="txtEnquiry_No"> @Model.Dealer.Contact_No_2</label>

                                                @*</div>*@

                                            </td>

                                        </tr>

                                        <tr>

                                            <th style="padding: 5px; text-align: left; width:15%">Email : </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                @*<div class="form-group">*@

                                                <label style="color: #333" for="txtEnquiry_Date"> @Model.Dealer.Email</label>

                                                @*</div>*@

                                            </td>

                                        </tr>

                                        @*<tr>

                                            <th style="padding: 5px; text-align: left; width:15%">Dealer Percentage Share : </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                <div class="form-group">

                                                <label style="color: #333" for="txtProduct_Name"> @Model.Dealer.Dealer_Percentage_Share</label>

                                                </div>

                                            </td>

                                        </tr>

                                        <tr>

                                            <th style="padding: 5px; text-align: left; width:15%">Brand Percentage Share : </th>

                                            <td style="padding: 10px; text-align: left; width:85%">

                                                <div class="form-group">

                                                <label style="color: #333" for="txtQuantity"> @Model.Dealer.Brand_Percentage_Share</label>

                                                </div>

                                            </td>

                                        </tr>*@

                                    </table>

                                    <div class="btn-group">

                                        <input type="hidden" id="hdnDealerId" value="@Model.Filter.Dealer_Id" name="Filter.Dealer_Id" class="auto-complete-value" />

                                        <input type="hidden" id="hdnBrandId" value="@Model.Filter.Brand_Id" name="Filter.Brand_Id" />

                                    </div>

                                </form>
                            </div>

                        </div>

                    </div>
                    <!-- /.box-body -->

                </div>
                <!-- /.box -->

            </div>

        </div>



    </div>
    <!-- /.content-wrapper -->
