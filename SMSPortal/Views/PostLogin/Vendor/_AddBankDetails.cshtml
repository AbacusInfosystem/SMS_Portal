<script type="text/javascript">
    $(document).ready(function () {

        $('input:not(.non-iCheck input:checkbox)').iCheck({
            checkboxClass: 'icheckbox_square-green',
            radioClass: 'iradio_square-green',
            increaseArea: '20%' // optional
        });


    });
</script>
