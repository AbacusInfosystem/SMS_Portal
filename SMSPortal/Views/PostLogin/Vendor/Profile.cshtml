@model SMSPortal.Models.PostLogin.VendorViewModel
@using SMSPortalInfo.Common

<script src="~/Scripts/Module/Vendor/vendor_profile_methods.js"></script>
<script src="~/Scripts/Module/Vendor/vendor_profile_events.js"></script>

<div class="content-wrapper">
    @*@{Html.RenderPartial("_ServerMessage");}*@
    <section class="content-header">
        <h1>
            Vendor Profile
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>

            <li class="active">Profile</li>

            <li class="active">Profile Details</li>

        </ol>
    </section>
    <form id="frmVendorProfileMaster">
        <input type="hidden" id="hdnVendor_Id" name="Vendor.Vendor_Id" value="@Model.Vendor.Vendor_Id" />
        <section class="content">
            <div class="box box-primary">
                <div class="row">
                    <div class="col-md-12">
                        <div class="box-header with-border">
                            <i class="fa fa-chevron-left" style="margin-right: 5px; cursor: pointer;font-size:22px; "></i>
                            <h3 class="box-title">Profile Details</h3>
                        </div>
                        <div class="box-body">
                            <div class="box-body">
                                <div class="row">
                                    <div class="col-md-4">
                                        <div class="row">
                                            <div class="col-md-8">
                                                    <div class="form-group">
                                                        <label>Name :</label>
                                                        <label style="font-size:12px">@Model.Vendor.Vendor_Name</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    <label>Address :</label>
                                                    <label style="font-size:12px">@Model.Vendor.Address</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-10">
                                                <div class="form-group">
                                                    <label>Email :</label>
                                                    <label style="font-size:12px">@Model.Vendor.Email</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    <label>state :</label>
                                                    <label style="font-size:12px">@Model.Vendor.State</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    <label>City :</label>
                                                    <label style="font-size:12px">@Model.Vendor.City</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    <label>Pincode :</label>
                                                    <label style="font-size:12px">@Model.Vendor.Pincode</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-10">
                                                <div class="form-group">
                                                    <label>Contact No1 :</label>
                                                    <label style="font-size:12px">@Model.Vendor.Contact_No_1</label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-8">
                                                <div class="form-group">
                                                    <label>Contact No2 :</label>
                                                    <label style="font-size:12px">@Model.Vendor.Contact_No_2</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="box-footer">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="btn-group pull-left">
                                                <button type="button" id="btnEdit" class="btn btn-warning btn-sm">Edit</button>
                                                <button type="button" id="btnAddBankDetails" class="btn btn-warning btn-sm" data-toggle="modal" data-target="#div_Parent_Modal_Fade">Add Bank Details </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </form>
</div>