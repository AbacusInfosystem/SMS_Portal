@model SMSPortal.Models.PostLogin.PayableViewModel
@using SMSPortalInfo.Common
<link href="~/Content/AdminLTE/plugins/daterangepicker/daterangepicker-bs3.css" rel="stylesheet" type="text/css" />
<script src="~/Content/AdminLTE/plugins/daterangepicker/daterangepicker.js" type="text/javascript"></script>
<script src="~/Content/AdminLTE/plugins/input-mask/jquery.inputmask.js" type="text/javascript"></script>
<script src="~/Content/AdminLTE/plugins/input-mask/jquery.inputmask.date.extensions.js" type="text/javascript"></script>
<script src="~/Content/AdminLTE/plugins/input-mask/jquery.inputmask.extensions.js" type="text/javascript"></script>
<script src="~/Scripts/Module/Payables/Payables_events.js"></script>
<script src="~/Scripts/Module/Payables/Payables_Methods.js"></script>

<script type="text/javascript">

    $(document).ready(function () {

        $('.iradio-list').iCheck({
            radioClass: 'iradio_square-green',
            increaseArea: '20%' // optional
        });

    });
</script>
<div class="content-wrapper">
    @*@{Html.RenderPartial("_ServerMessage");}*@
    <section class="content-header">
        <h1>
            Payable Master
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>

            <li class="active">Admin</li>

            <li class="active">Payable</li>

        </ol>
    </section>

    <form id="frmPayableMaster">
        <input type="hidden" id="hdnPayable_Id" value="@Model.Payable.Payable_Id" name="Payable.Payable_Id" />
        <input type="hidden" id="hdnPayable_Item_Id" value="" name="" />

        <section class="content">
            <div class="box box-primary">
                <div class="row">
                    <div class="col-md-12">

                        <div class="box-header with-border">
                            <i class="fa fa-chevron-left" style="margin-right: 5px; cursor: pointer;font-size:22px; "></i>
                            <h3 class="box-title">Payables </h3>
                        </div>

                        <div class="box-body">

                            <div id="dvInvoice" class="row">
                                <div class="col-md-3">
                                    <div class="form-group auto-complete">
                                        <label for="InvoiceNo" class="lookup-title">Invoice No</label>
                                        <div class="input-group">
                                            <input type="text" class="form-control input-sm autocomplete-text" name="Payable.Invoice_No" id="txtInvoiceNo" placeholder="Enter Invoice No" value="@Model.Payable.Invoice_Id" data-table="Invoice" data-col="Invoice_Id,Invoice_No" data-headernames="Invoice No">
                                            <input type="hidden" id="hdnInvoiceId" value="@Model.Payable.Invoice_Id" name="Payable.Invoice_Id" class="auto-complete-value" />
                                            <input type="hidden" id="hdnInvoiceNo" value="@Model.Payable.Invoice_No" name="Payable.Invoice_No" class="auto-complete-label" />
                                            <div class="input-group-addon">
                                                <a href="#" class="text-muted lookup-btn" id="hrefInvoice" role="button"><i class="fa fa-server"></i></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-2">
                                    <div class="form-group">
                                        <label style="color: #333">Amount</label>
                                        <input readonly type="text" class="form-control input-sm" name="Payable.Invoice_Amount" id="txtInvoice_Amount" placeholder="Enter Invoice Amount" value="@Model.Payable.Invoice_Amount" maxlength="20">
                                    </div>
                                </div>

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label for="txtPayDate" style="color: #333">Payable Date</label>
                                        <div class="input-group">
                                            <div class="input-group-addon">
                                                <i class="fa fa-calendar"></i>
                                            </div>
                                            <input type="text" class="form-control  input-sm" id="txtPayDate" data-inputmask="'alias': 'dd/mm/yyyy'" data-mask name="Payable.Payable_Date" placeholder="Enter Payable Date" value="@Model.Payable.Payable_Date" />
                                            @*<input type="text" class="form-control  input-sm" id="txtBirthDate" name="candidateshortlist.Candidate_DOB" placeholder="Enter Birth Date" value="@((Model.candidateshortlist.Candidate_DOB == DateTime.MinValue ? "" : Model.candidateshortlist.Candidate_DOB.ToString("dd-mm-yyyy")))" />*@
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="row">

                                <div class="col-md-3">
                                    <div class="form-group">
                                        <label style="color: #333">Transaction Type</label>
                                        <select id="drpTransaction" class="form-control" name="">
                                            <option value="">-Select Transaction Type-</option>
                                            @foreach (var item in LookupInfo.Get_Transaction_Types())
                                            {
                                                if (Model.Payable.Transaction_Type == item.Key)
                                                {
                                            <option value="@item.Key" selected="selected">@item.Value</option>
                                                }
                                                else
                                                {
                                            <option value="@item.Key">@item.Value</option>
                                                }
                                            }
                                        </select>
                                    </div>
                                </div>

                            </div>

                            <div class="box-body" style="display:none" id="divCheque">
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label for="txtPayDate" style="color: #333">Cheque Date</label>
                                            <div class="input-group">
                                                <div class="input-group-addon">
                                                    <i class="fa fa-calendar"></i>
                                                </div>
                                                <input type="text" class="form-control  input-sm" id="txtChequeDate" data-inputmask="'alias': 'dd/mm/yyyy'" data-mask name="Payable.ChequeDate" placeholder="Enter Cheque Date" value="@Model.Payable.Cheque_Date" />

                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">

                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label style="color: #333">Amount</label>
                                                <input type="text" class="form-control input-sm" name="Payable.Payable_Item_Amount" id="txtPayable_Item_Amount2" placeholder="Enter IFSC Code" value="@Model.Payable.Payable_Item_Amount" maxlength="20">
                                            </div>
                                        </div>

                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label style="color: #333">Cheque No</label>
                                            <input type="text" class="form-control input-sm" name="Payable.ChequeNo" id="txtChequeNo" placeholder="Enter Cheque No" value="@Model.Payable.Cheque_Number" maxlength="20">
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label style="color: #333">Bank Name</label>
                                            <input type="text" class="form-control input-sm" name="Payable.BankName" id="txtBankName" placeholder="Enter Bank Name" value="@Model.Payable.Bank_Name" maxlength="20">
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <div class="form-group">
                                            <label style="color: #333">IFSC Code</label>
                                            <input type="text" class="form-control input-sm" name="Payable.IFSC_Code" id="txtIFSCCode" placeholder="Enter IFSC Code" value="@Model.Payable.IFSC_Code" maxlength="20">
                                        </div>
                                    </div>
                                    <div class="col-md-2">
                                        <br />
                                        <div class="btn-group">
                                            <button type="submit" id="btnSave" class="btn btn-success btn-sm">Save</button>
                                            <button type="reset" id="btnReset" class="btn btn-warning btn-sm">Reset</button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="box-body" style="display:none" id="divNEFT">
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label style="color: #333">NEFT</label>
                                            <input type="text" class="form-control input-sm" name="Payable.NEFT" id="txtNEFT" placeholder="Enter NEFT Details" value="@Model.Payable.NEFT" maxlength="20">
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label style="color: #333">Amount</label>
                                                <input type="text" class="form-control input-sm" name="Payable.Payable_Item_Amount" id="txtPayable_Item_Amount" placeholder="Enter IFSC Code" value="@Model.Payable.Payable_Item_Amount" maxlength="20">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <br />
                                        <div class="btn-group">
                                            <button type="submit" id="btnSave1" class="btn btn-success btn-sm">Save</button>
                                            <button type="reset" id="btnReset" class="btn btn-warning btn-sm">Reset</button>
                                        </div>

                                    </div>
                                </div>
                            </div>

                            <div class="box-body" style="display:none" id="divCredit_Debit">
                                <div class="row">
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label style="color: #333">Amount</label>
                                            <input type="text" class="form-control input-sm" name="Payable.Payable_Item_Amount" id="txtPayable_Item_Amount1" placeholder="Enter IFSC Code" value="@Model.Payable.Payable_Item_Amount" maxlength="20">
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="form-group">
                                            <label style="color: #333">Credit/Debit Card</label>
                                            <input type="text" class="form-control input-sm" name="Payable.Credit_Debit_Card" id="txtCredit_Debit" placeholder="Enter Credit Debit Card Details" value="@Model.Payable.Credit_Debit_Card" maxlength="20">
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <br />
                                        <div class="btn-group">
                                            <button type="submit" id="btnSave2" class="btn btn-success btn-sm">Save</button>
                                            <button type="reset" id="btnReset" class="btn btn-warning btn-sm">Reset</button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="box-footer">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="btn-group pull-right">
                                            <button type="button" id="btnEdit" style="display:none;" class="btn btn-warning btn-sm">Edit </button>
                                            <button type="button" id="btnDelete" style="display:none;" class="btn btn-warning btn-sm">Delete </button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12">
                    <div class="box box-solid">
                        <div class="box-body table-responsive no-padding">
                            <table class="table table-striped" id="tblPayableItems">
                                <tr id="subhead">
                                    <th></th>
                                    <th>Amount</th>
                                    <th>Transaction Type</th>
                                    <th>Bank Name</th>
                                    <th>IFSC Code</th>
                                    <th>Cheque</th>
                                    <th>Cheque No</th>
                                    <th>Transaction Details</th>
                                </tr>
                            </table>
                        </div>
                        <div class="box-footer clearfix">
                            <div class="pagination pagination-sm no-margin pull-right">
                                <ul></ul>
                            </div>
                            <div id="divSearchGridOverlay" class="overlay">
                                <i class="fa fa-refresh fa-spin"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </form>
</div>