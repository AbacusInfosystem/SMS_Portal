@model SMSPortal.Models.PostLogin.ReceivableViewModel

@using SMSPortal.Common;


<script src="~/Scripts/Module/Receivable/search_receivable_event.js"></script>

<script src="~/Scripts/Module/Receivable/search_receivable_method.js"></script>


<!-- Content Wrapper. Contains page content -->

<div class="content-wrapper">

    <!-- Content Header (Page header) -->

    @{Html.RenderPartial("_ServerMessage");}

    <section class="content-header">

        <h1>

            Dealer Payables Master

            <small></small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
            <li><a href="#"><i class="fa fa-dashboard"></i>Admin</a></li>
            <li class="active">Dealer Payables</li>
        </ol>
    </section>

    <div class="row">

        <div class="col-xs-12">

            <div class="box">

                <div class="box-body">

                    <div class="row">

                        <div class="col-md-12">

                            <form id="frmReceivable">

                                <table border="1" style="width:100%">

                                    <tr>

                                        <th style="padding: 5px; text-align: left; width:15%">Transaction Type : </th>

                                        <td style="padding: 10px; text-align: left; width:85%">

                                            @*<div class="form-group">*@

                                            <label style="color: #333" for="txtCustomer_Name">@Model.Receivable.Transaction_Type</label>

                                            @*</div>*@

                                        </td>

                                    </tr>

                                    <tr>

                                        <th style="padding: 5px; text-align: left; width:15%">Balance Amount : </th>

                                        <td style="padding: 10px; text-align: left; width:85%">

                                            @*<div class="form-group">*@

                                            <label style="color: #333" for="txtCustomer_Address">@Model.Receivable.Balance_Amount</label>

                                            @*</div>*@

                                        </td>

                                    </tr>

                                    <tr>

                                        <th style="padding: 5px; text-align: left; width:15%">Bank Name : </th>

                                        <td style="padding: 10px; text-align: left; width:85%">

                                            @*<div class="form-group">*@

                                            <label style="color: #333" for="txtTel_No">@Model.Receivable.Bank_Name</label>

                                            @*</div>*@

                                        </td>

                                    </tr>

                                    <tr>

                                        <th style="padding: 5px; text-align: left; width:15%">IFSC Code : </th>

                                        <td style="padding: 10px; text-align: left; width:85%">

                                            @*<div class="form-group">*@

                                            <label style="color: #333" for="txtEmail">@Model.Receivable.IFSC_Code</label>

                                            @*</div>*@

                                        </td>

                                    </tr>

                                    <tr>

                                        <th style="padding: 5px; text-align: left; width:15%">Cheque Number :  </th>

                                        <td style="padding: 10px; text-align: left; width:85%">

                                            @*<div class="form-group">*@

                                            <label style="color: #333" for="txtContact_Person">@Model.Receivable.Cheque_Number</label>

                                            @*</div>*@

                                        </td>

                                    </tr>

                                    <tr>

                                        <th style="padding: 5px; text-align: left; width:15%">Cheque Date : </th>

                                        <td style="padding: 10px; text-align: left; width:85%">

                                            @*<div class="form-group">*@

                                            <label style="color: #333" for="txtContact_Person_Tel_No"> @Model.Receivable.Cheque_Date</label>

                                            @*</div>*@

                                        </td>

                                    </tr>

                                    <tr>

                                        <th style="padding: 5px; text-align: left; width:15%">NEFT : </th>

                                        <td style="padding: 10px; text-align: left; width:85%">

                                            @*<div class="form-group">*@

                                            <label style="color: #333" for="txtContact_Person_Mobile_No"> @Model.Receivable.NEFT</label>

                                            @*</div>*@

                                        </td>

                                    </tr>

                                    <tr>

                                        <th style="padding: 5px; text-align: left; width:15%">Credit Debit Card : </th>

                                        <td style="padding: 10px; text-align: left; width:85%">

                                            @*<div class="form-group">*@

                                            <label style="color: #333" for="txtEnquiry_No"> @Model.Receivable.Credit_Debit_Card</label>

                                            @*</div>*@

                                        </td>

                                    </tr>

                                    <tr>

                                        <th style="padding: 5px; text-align: left; width:15%">Receivable Date : </th>

                                        <td style="padding: 10px; text-align: left; width:85%">

                                            @*<div class="form-group">*@

                                            <label style="color: #333" for="txtEnquiry_Date"> @Model.Receivable.Receivable_Date</label>

                                            @*</div>*@

                                        </td>

                                    </tr>

                                </table>

                                <div class="btn-group">

                                    <input type="hidden" id="hdnDealerId" value="@Model.Filter.Dealer_Id" name="Filter.Dealer_Id"/>

                                </div>

                            </form>
                        </div>

                    </div>

                </div>
                <!-- /.box-body -->

            </div>
            <!-- /.box -->

        </div>

    </div>



</div>

<!-- /.content-wrapper -->
